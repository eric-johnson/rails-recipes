<h1>Recipes</h1>
<p>This is a list of my recipes</p>

<ul>
<% @recipes.each do |recipe| %>
  <li><%= link_to recipe.name, recipe %>
  (<%= link_to 'edit', edit_recipe_path( recipe ) %>)
  (<%= link_to "delete", recipe, :method=>:delete %>)
<% end %>
</ul>

<%= link_to 'This is a button to make a new recipe', new_recipe_path %>